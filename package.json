{
  "name": "kerala-lt-line-break-detection",
  "version": "1.0.0",
  "description": "AI-powered real-time electrical line break detection for Kerala State Electricity Board",
  "main": "server/index.ts",
  "scripts": {
    "dev": "npm run build:client && npm run server:dev",
    "server:dev": "tsx watch server/index.ts",
    "client:dev": "vite --config client/vite.config.ts",
    "build": "npm run build:client",
    "build:client": "vite build --config client/vite.config.ts",
    "start": "node start.js",
    "test": "jest",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:studio": "drizzle-kit studio",
    "db:setup": "node scripts/setup-database.js",
    "setup": "node setup-config.js",
    "check-config": "node scripts/simple-check.js",
    "setup-windows": "setup-windows.bat",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "server:simple": "node server-simple.js",
    "dev:simple": "npm run server:simple"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "leaflet": "^1.9.4",
    "wouter": "^2.12.1",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "zod": "^3.22.4",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "keywords": [
    "kerala",
    "electricity",
    "line-break",
    "detection",
    "ai",
    "ml",
    "react",
    "nodejs",
    "typescript"
  ],
  "author": "KSEBL Development Team",
  "license": "MIT"
}